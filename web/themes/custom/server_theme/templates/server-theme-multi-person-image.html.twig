<div class = "multi-person-card">
	<div class = "container">
		{% for j in 0..1 %}
		<div class = "row-{{ j }} row" >
				{% for i in 0..4 %}
				<div class = "col mb-2">
					<div class = "single-person-card">
						<div class = "user-info">
							<div class="w-32 user-image-wrapper">
								{% if url %}
									<a href="{{ url }}">
								{% endif %}
								<div class="rounded-full overflow-hidden h-32 mb-4">
									{% if image %}
										<img src="{{ image }}" alt="{{ image_alt }}" class="object-cover w-full" />
									{% else %}
										<div class="flex w-full h-full justify-center items-center bg-gray-500 text-white font-bold text-4xl">
											{{ initials }}
										</div>
									{% endif %}
								</div>
							 
								
								 
								{% if url %}
									</a>
								{% endif %}
							</div>
							<div class="user-details">
								<div class = "name">
									{{ name }}
								</div>
									
								<div class = "designation">
								{{ designation }}
								</div>
								<div class = "role">
								{{ role }}
								</div>
								
								</div>
						</div>
						<div class = "contact-info">
							<div class="bottom-left">
								<span class = "email"><i class="fa fa-envelope" aria-hidden="true">{{ email }}</i></span>
							</div>
							<div class="bottom-right">
								<span class = "phone"> <i class="fa fa-phone" aria-hidden="true">{{ phone }}</i></span>
							</div>
							
						</div>
						</div>
				</div>
				{% endfor %}
		</div>
	{% endfor %}
	</div>
	
</div>
